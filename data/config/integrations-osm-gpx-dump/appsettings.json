{
    "OutputFile": "/var/app/data/osm-gpx-latest.fgb",
    "Serilog": {
      "Using": [
        "Serilog.Sinks.File",
        "Serilog.Sinks.Console"
      ],
      "MinimumLevel": "Verbose",
      "WriteTo": [
        {
          "Name": "File",
          "Args": {
            "path": "logs/log-.txt",
            "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog",
            "rollingInterval": "Day"
          }
        },
        {
          "Name": "Console"
        }
      ]
    }
  }